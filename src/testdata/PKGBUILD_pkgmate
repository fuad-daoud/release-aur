pkgname=pkgmate-bin
pkgver=v0.0.0-test-release-aur
pkgrel=1
pkgdesc="TUI application to manage your dependencies"
arch=('x86_64')
url="https://github.com/fuad-daoud/pkgmate"
license=('MIT')
provides=()
conflicts=()
source_x86_64=(
"pkgmate-bin-v0.0.0-test-release-aur-x86_64::https://github.com/fuad-daoud/pkgmate/releases/download/v0.0.0-test-release-aur/pkgmate-linux-amd64"
)

sha256sums_x86_64=(
'ce9b515fd45526ac641d7bcf8520f58a72dc82d8109cfe3b317484cd837c5cec'
)


package() {
    if [ "$CARCH" = "x86_64" ]; then
        install -Dm755 "$srcdir/$pkgname-$pkgver-x86_64" "$pkgdir/usr/bin/pkgmate"
    fi
}

